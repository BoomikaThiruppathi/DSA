import java.io.*;
import java.util.*;

public class Solution {
public static void PI(int m,int n){
    if(m > n){
        return;
    }
    System.out.println(m);
    PI(m + 1,n);
}
    public static void main(String[] args) {
    Scanner scn = new Scanner(System.in);
        int m = 1;
        int n = scn.nextInt();
        PI(m,n);
    }
}
